{
    "compilerOptions": {
        "outDir": "dist",
        "lib": ["es2019", "DOM", "DOM.Iterable"],
        "target": "es2016",
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "sourceMap": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "allowJs": true,
        "strict": true,
        "moduleResolution": "node",
        "resolveJsonModule": true,
   }
} 