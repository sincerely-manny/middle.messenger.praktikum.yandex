{
    "compilerOptions": {
        "outDir": "dist",
        "lib": [
            "es2017",
            "DOM",
            "DOM.Iterable",
        ],
        "target": "es2016",
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "sourceMap": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "allowJs": true,
        "strict": true,
        "moduleResolution": "node",
        "resolveJsonModule": true,
   }
} 